import java.util.Scanner;

public class senha3x {
	public static void main(String[] args) {

		System.out.println("digite sua senha vocÃª tem 3 tentativas");
		Scanner sc = new Scanner(System.in);
		int senha = sc.nextInt();

		for (int i = 0; i < 3 ; i = i++) {
			if (senha != 1822) {
				System.out.println("senha errada!");
				sc.nextInt();
				System.out.println("senha errada!");
				sc.nextInt();
				System.out.println("tentativas excedidas!");

				break;
			}

			System.out.println("senha correta! bem vindo!");
			break;
		}

		sc.close();

	}
}
